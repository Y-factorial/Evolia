#news-container {
    height: 0px;
    transition-property: height;
    transition-duration: 0.5s;
    transition-timing-function: ease-in-out;
    
    overflow: hidden;
    
}

#news-background {
    background-color: rgba(0, 0, 0, 0.75);
    margin-right: 10px;
    margin-bottom: 10px;
    margin-left: 220px;
    
    padding: 4px;
}

#news-message-box
{
    overflow: hidden;
}

#news-message {
    margin: 0;
    padding: 2px 4px 4px 4px;
    font-size: 24px;
    color: rgb(255, 255, 255);
