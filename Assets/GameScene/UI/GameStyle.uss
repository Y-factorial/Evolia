#header {
    background-color: rgb(255, 187, 68);
    padding: 5px 5px;
    flex-direction: row;
    flex-wrap: wrap;
    align-items: flex-start;
    font-size: 24px;
    -unity-font-style: bold;
}


#planet-info {
    flex-grow: 1;
    flex-shrink: 0;
    flex-direction: row;
    flex-wrap: nowrap;
    align-content: stretch;
    align-items: center;
}

#hamburger-button {
    margin: 0;
    padding: 5px;
    width: 50px;
    height: 50px;
    background-size: 40px;
    background-repeat: no-repeat;
    background-color: transparent;
    background-position: center;
    border-width: 0;
    background-image: url("project://database/Assets/GameScene/Sprites/ControlButton.png#ControlButton_4");
}

#hamburger-button:hover {
    background-image: url("project://database/Assets/GameScene/Sprites/ControlButton.png#ControlButton_12");
}

#hamburger-button:active {
    background-image: url("project://database/Assets/GameScene/Sprites/ControlButton.png#ControlButton_12");
}

#planet-name-label {
    flex-grow: 1;
    background-color: rgb(238, 170, 51);
    padding-left: 10px;
    padding-right: 10px;
    height: 40px;
    width: 200px;
    max-width: 320px;
    -unity-text-align: middle-left;
}

#planet-age-label {
    flex-grow: 1;
    background-color: rgb(238, 170, 51);
    background-image: url("project://database/Assets/GameScene/Sprites/AgeIcon.png#AgeIcon");
    background-size: 32px 32px;
    background-repeat: no-repeat;
    background-position: 4px 4px;
    padding-left: 40px;
    padding-right: 10px;
    height: 40px;
    width: 260px;
    max-width: 320px;
    -unity-text-align: middle-right;
}

#speed-button-group {
    flex-grow: 0;
    flex-shrink: 0;
    flex-direction: row;
    flex-wrap: nowrap;
    align-content: stretch;
    align-items: center;
}

.speed-button {
    margin: 0;
    padding: 5px;
    width: 50px;
    height: 50px;
    background-size: 40px;
    background-repeat: no-repeat;
    background-color: transparent;
    background-position: center;
    border-width: 0;
}

#speed-button-Pause {
    background-image: url("project://database/Assets/GameScene/Sprites/ControlButton.png#ControlButton_0");
}

#speed-button-Pause:hover,
#speed-button-Pause:active,
#speed-button-Pause.active {
    background-image: url("project://database/Assets/GameScene/Sprites/ControlButton.png#ControlButton_8");
}

#speed-button-Normal {
    background-image: url("project://database/Assets/GameScene/Sprites/ControlButton.png#ControlButton_1");
}

#speed-button-Normal:hover,
#speed-button-Normal:active,
#speed-button-Normal.active {
    background-image: url("project://database/Assets/GameScene/Sprites/ControlButton.png#ControlButton_9");
}

#speed-button-Fast {
    background-image: url("project://database/Assets/GameScene/Sprites/ControlButton.png#ControlButton_2");
}

#speed-button-Fast:hover,
#speed-button-Fast:active,
#speed-button-Fast.active {
    background-image: url("project://database/Assets/GameScene/Sprites/ControlButton.png#ControlButton_10");
}

#speed-button-Faster {
    background-image: url("project://database/Assets/GameScene/Sprites/ControlButton.png#ControlButton_3");
}

#speed-button-Faster:hover,
#speed-button-Faster:active,
#speed-button-Faster.active {
    background-image: url("project://database/Assets/GameScene/Sprites/ControlButton.png#ControlButton_11");
}

#speed-button-SuperFast {
    background-image: url("project://database/Assets/GameScene/Sprites/ControlButton.png#ControlButton_5");
}

#speed-button-SuperFast:hover,
#speed-button-SuperFast:active,
#speed-button-SuperFast.active {
    background-image: url("project://database/Assets/GameScene/Sprites/ControlButton.png#ControlButton_13");
}



#environment-info {
    flex-shrink: 0;
    flex-grow: 10;
    flex-direction: row;
    flex-wrap: nowrap;
}

.planet-environment-label {
    flex-grow: 1;
    border-width: 2px;
    border-left-color: rgb(51, 34, 17);
    border-top-color: rgb(51, 34, 17);
    border-right-color: rgb(153, 102, 51);
    border-bottom-color: rgb(153, 102, 51);
    background-color: rgb(255, 221, 119);
    background-size: 32px 32px;
    background-repeat: no-repeat;
    background-position: 4px 4px;
    padding-left: 40px;
    padding-right: 10px;
    height: 40px;
    width: 160px;
    max-width: 240px;
    -unity-text-align: middle-right;
}

#planet-temperature-label {
    background-image: url("project://database/Assets/GameScene/Sprites/EnvironmentIcon.png#EnvironmentIcon_0");
}

#planet-pressure-label {
    background-image: url("project://database/Assets/GameScene/Sprites/EnvironmentIcon.png#EnvironmentIcon_1");
    padding-right: 36px;
}

.pressure-unit {
    position: relative;
    left: 34px;
    background-size: 32px 32px;
    background-repeat: no-repeat;
    background-position: 100% 0;
    background-image: url("project://database/Assets/GameScene/Sprites/PressureUnit.png#PressureUnit");
}

#planet-humidity-label {
    background-image: url("project://database/Assets/GameScene/Sprites/EnvironmentIcon.png#EnvironmentIcon_5");
}

#planet-o2-label {
    background-image: url("project://database/Assets/GameScene/Sprites/EnvironmentIcon.png#EnvironmentIcon_2");
}

#planet-co2-label {
    background-image: url("project://database/Assets/GameScene/Sprites/EnvironmentIcon.png#EnvironmentIcon_3");
}

.planet-environment-label.ppm {
    padding-right: 36px;
}

.ppm .ppm-unit {
    position: relative;
    left: 34px;
    background-size: 32px 32px;
    background-repeat: no-repeat;
    background-position: 100% 0;
    background-image: url("project://database/Assets/GameScene/Sprites/PPMUnit.png#PPMUnit");
}

#content {
    flex-grow: 1;
    justify-content: center;
}

#saving-throbber {
    position: absolute;
    margin: 20px;
    width: 64px;
    height: 64px;
    background-image: url("project://database/Assets/GameScene/Sprites/Throbber.png#Throbber");
    background-size: cover;
    rotate: 0;
    opacity: 0;
    transition-property: opacity;
    transition-duration: 0.25s;
    transition-timing-function: ease-in-out;
}

#debug-panel {
    display: none;
    position: absolute;
    top: 0;
    left: 0;
    flex-grow: 0;
    width: 400px;
    flex-direction: column;
    font-size: 16px;
}

.debug-row {
    flex-direction: row;
}

.debug-key {
    background-color: rgba(0, 0, 0, 0.5);
    padding: 5px;
    color: white;
}

.debug-value {
    background-color: rgba(0, 0, 0, 0.5);
    padding: 5px;
    color: white;
}

.debug-key {
    width: 100px;
    -unity-text-align: middle-center;
}

.debug-value {
    width: 200px;
    -unity-text-align: middle-left;
}

#minimap {
    position: absolute;
    top: 0;
    right: 0;
    margin: 10px;
    flex-grow: 0;
    flex-shrink: 0;
    border-width: 8px;
    border-left-color: rgb(153, 102, 51);
    border-top-color: rgb(153, 102, 51);
    border-right-color: rgb(51, 34, 17);
    border-bottom-color: rgb(51, 34, 17);
    background-color: rgb(255, 221, 119);
    width: 416px;
    height: 216px;
}

#content-top-container
{
    flex-grow: 1;
}

#menu-container
{
    margin-bottom: 20px;
}

#menu-panel {
    flex-grow: 0;
    align-self: flex-start;
    flex-direction: column;
    margin-left: 20px;
    border-width: 8px;
    border-left-color: rgb(153, 102, 51);
    border-top-color: rgb(153, 102, 51);
    border-right-color: rgb(51, 34, 17);
    border-bottom-color: rgb(51, 34, 17);
    background-color: rgb(255, 221, 119);
}

.submenu-panel {
    flex-grow: 0;
    align-self: flex-start;
    flex-direction: column;
    margin-left: 20px;
    border-width: 8px;
    border-left-color: rgb(153, 102, 51);
    border-top-color: rgb(153, 102, 51);
    border-right-color: rgb(51, 34, 17);
    border-bottom-color: rgb(51, 34, 17);
    background-color: rgb(255, 221, 119);
}

.submenu-header {
    background-color: rgb(255, 187, 68);
    padding-left: 10px;
}
.submenu-label {
    -unity-font-style: bold;
    font-size: 20px;
}

.submenu-content {
    flex-direction: row;
}

* .menu-button {
    margin: 10px;
    padding: 0px;
    width: 64px;
    height: 64px;
    background-size: 64px 64px;
    background-repeat: no-repeat;
    background-color: transparent;
    border-width: 0;
}

* .menu-button:hover,
* .menu-button:active,
* .menu-button.active {
    background-color: rgb(255, 187, 68);
}

#search-button {
    background-image: url("project://database/Assets/GameScene/Sprites/MenuIcon.png#MenuIcon_3");
}

